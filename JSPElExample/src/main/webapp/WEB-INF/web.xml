<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JSPElExample</display-name>
  
  <context-param>
		<param-name>state</param-name>
		<param-value>KA</param-value>
	</context-param>
	<context-param>
		<param-name>city</param-name>
		<param-value>Blore</param-value>
	</context-param>
	
  <welcome-file-list>

		<welcome-file>test.jsp</welcome-file>

	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>TestServlet</display-name>
		<servlet-name>TestServlet</servlet-name>
		<servlet-class>com.simplilearn.servlets.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/test.jlc</url-pattern>
	</servlet-mapping>
</web-app>